    "eslint-plugin-svelte": "^2.30.0",
    "postcss": "^8.4.35",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.10.1",
    "svelte": "^4.2.11",
    "svelte-check": "^3.6.0",
